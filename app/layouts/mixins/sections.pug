mixin item(avatar="assets/i/services/service-no-photo.png", title="Title for service", description, provider, unit, provider_title="Provider:", unit_title="Payment token:", unit_alert, unit_alert_text="You need to approve DAI tokens for Robonomics factory smart contract")
	

	.section-light.item
		a.item-avatar(href="#")
			span.item-avatar--image(style="background-image: url('"+avatar + " ')")
		.item-content
			h2
				a(href="#")=title
			if description
				.t-hyphen=description

			if provider || unit
				.item-bottom
					if provider
						.item-bottom--line
							span=provider_title
							span.item-bottom--info=provider
					if unit
						.item-bottom--line
							span=unit_title
							span.item-bottom--info=unit

							if unit_alert
								a.js-tooltip.icon-round-sm.icon-alert(href="#", data-tooltip=unit_alert_text)
									i.i-alert
		


mixin sidebar(navActive, multilang, logolight='assets/i/logo-dapp-sign.svg', logodark='assets/i/logo-dapp-sign.svg', project='robonomics')
	
	.sidebar.flexcols
		.sidebar-col.sidebar-col--sm
			.sidebar-col-in
				section
					a(href="/")
						img.theme-day(alt="dApp logo", src=logolight)
						img.theme-night(alt="dApp logo", src=logodark)

				section
					a(href="javascript:;", on-toggle="#sidebar-nav1").sidebar-i--lg.js-sidebar-link
						i.i-menu

					if multilang
						a(href="javascript:;", on-toggle="#sidebar-lang").sidebar-i--lg.js-sidebar-link EN

					#themeMode
						a(href="javascript:;", @click='toggleTheme()').sidebar-i--lg
							i.i-day(v-show="!nightMode")
							i.i-night(v-show="nightMode")


					script(src='https://cdn.jsdelivr.net/npm/vue')

					script.
						new Vue({
						    el: '#themeMode',
						    data: {
						        nightMode: false
						    },
						    methods:{
						        toggleTheme: function(){
						            this.nightMode = !this.nightMode;

						            if ( this.nightMode ){
						            	document.body.classList.remove('theme-light');
						            	document.body.classList.add('theme-dark');
						            }
						            if ( !this.nightMode ){
						            	document.body.classList.add('theme-light');
						            	document.body.classList.remove('theme-dark');
						            }

						        }
						    }
						});

				section.sidebar-col--bottom
					a(href="javascript:;", on-toggle="#sidebar-user").sidebar-i--lg.js-sidebar-link
						i.i-user
					a(href="javascript:;", on-toggle="#sidebar-info").sidebar-i--lg.js-sidebar-link ?


		
		if logolight == 'assets/i/logo-dapp-sign.svg'
			.sidebar-col.js-sidebar-content#sidebar-nav1
				.sidebar-col-in 
					section
						a(href="/")
							img.theme-day(alt="dApp logo", src="assets/i/logo-dapp-text-blue.svg")
							img.theme-night(alt="dApp logo", src="assets/i/logo-dapp-text.svg")

					section
						nav.nav-vertical
							if navActive == 0
								a(href="#").active
									span.i-app.align-vertical
									span.align-vertical Services
							else
								a(href="#")
									span.i-app.align-vertical
									span.align-vertical Services

							if navActive == 1
								a(href="#").active
									span.i-lighthouse.align-vertical
									span.align-vertical Lighthouses
							else
								a(href="#")
									span.i-lighthouse.align-vertical
									span.align-vertical Lighthouses

							if navActive == 2
								a(href="#").active
									span.i-piechart.align-vertical
									span.align-vertical Statistics
							else
								a(href="#")
									span.i-piechart.align-vertical
									span.align-vertical Statistics

							if navActive == 3
								a(href="#").active
									span.i-transfer.align-vertical
									span.align-vertical AIRA to XRT
							else
								a(href="#")
									span.i-transfer.align-vertical
									span.align-vertical AIRA to XRT
		else
			.sidebar-col-padding.sidebar-col.js-sidebar-content#sidebar-nav1
				.sidebar-col-in 
					section
						nav.nav-vertical
							if navActive == 0
								a(href="#").active
									span.i-app.align-vertical
									span.align-vertical Services
							else
								a(href="#")
									span.i-app.align-vertical
									span.align-vertical Services

							if navActive == 1
								a(href="#").active
									span.i-lighthouse.align-vertical
									span.align-vertical Lighthouses
							else
								a(href="#")
									span.i-lighthouse.align-vertical
									span.align-vertical Lighthouses

							if navActive == 2
								a(href="#").active
									span.i-piechart.align-vertical
									span.align-vertical Statistics
							else
								a(href="#")
									span.i-piechart.align-vertical
									span.align-vertical Statistics

							if navActive == 3
								a(href="#").active
									span.i-transfer.align-vertical
									span.align-vertical AIRA to XRT
							else
								a(href="#")
									span.i-transfer.align-vertical
									span.align-vertical AIRA to XRT

		.sidebar-col.sidebar-col-padding.js-sidebar-content#sidebar-lang
			.sidebar-col-in
				section
					nav.nav-vertical
						a(href="#").active EN - English
						a(href="#") RU - Русский

		.sidebar-col.sidebar-col-padding.js-sidebar-content#sidebar-theme
			.sidebar-col-in
				section
					nav.nav-vertical
						a(href="#").active
							span.i-day.align-vertical
							span.align-vertical Light

						a(href="#")
							span.i-night.align-vertical
							span.align-vertical Dark


		.sidebar-col.sidebar-col-padding.js-sidebar-content#sidebar-user
			.sidebar-col-in
				section

					a(href="#").nowrap
						+avatar('avatar-small align-vertical m-r-10')
						b 0x40c5...c8d3

					.m-t-10.label.label-violet Sidechain
					

				section
					.disabled.js-tooltip(data-tooltip="Not available for Sidechain")
						b.m-r-10 2,100
						span.t-sm AIRA
					hr/
					.disabled.js-tooltip(data-tooltip="Not available for Sidechain")
						b.m-r-10 0
						span.t-sm AIRA ID
					hr/
					div
						b.m-r-10 0
						span.t-sm XRT
						



		if project == 'robonomics'
			.sidebar-col.sidebar-col-padding.js-sidebar-content#sidebar-info
				.sidebar-col-in
					section
						nav.nav-vertical
							a(href="https://github.com/airalab", target="_blank")
								span.i-github.align-vertical
								span.align-vertical Github

							a(href="https://www.reddit.com/r/robonomics/", target="_blank")
								span.i-reddit.align-vertical
								span.align-vertical Reddit

							a(href="https://robonomics.network/", target="_blank")
								span.i-cursor.align-vertical
								span.align-vertical robonomomics.network

							a(href="https://aira.life/", target="_blank")
								span.i-cursor.align-vertical
								span.align-vertical aira.life

		if project == 'ipci'
			.sidebar-col.sidebar-col-padding.js-sidebar-content#sidebar-info
				.sidebar-col-in
					section
						p
							a(href="https://ipci.io/", target="_blank") DAO IPCI
							br/
							| Blockchain technology for carbon markets, environmental assets and liabilities
						p Works with 
							a(href="https://robonomics.network/", target="_blank") Robonomomics.network


		





